{
  "name": "my-hai",
  "version": "0.0.0",
  "description": "React and Jest are ready to go!",
  "main": "application.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaphaelDeLaGhetto/rails-tutorial-template.git"
  },
  "keywords": [
    "archiver",
    "hai"
  ],
  "author": "Daniel Bidulock",
  "license": "MIT",
  "devDependencies": {
    "browserify-incremental": "^2.0.0",
    "jest-cli": "^0.4.0",
    "jquery": "^2.1.3",
    "react": "^0.13.1",
    "react-tools": "^0.13.1",
    "reactify": "^1.1.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "node ./node_modules/jest-cli/bin/jest.js"
